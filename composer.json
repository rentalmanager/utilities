{
    "name": "rentalmanager/utilities",
    "type": "library",
    "description": "Rental Manager Utilities package which propels the basic system where you can manage properties for rent.",
    "keywords": [
        "Rentbits",
        "Utilities"
    ],
    "homepage": "https://gitlab.com/rentalmanager/utilities",
    "license": "proprietary",
    "authors": [
        {
            "name": "Rentbits",
            "email": "support@rentbits.com",
            "homepage": "https://rentbits.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1.3",
        "kkszymanowski/traitor": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "RentalManager\\Utilities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RentalManager\\Utilities\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RentalManager\\Utilities\\UtilitiesServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
